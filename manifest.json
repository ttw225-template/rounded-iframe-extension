{
  "manifest_version": 3,
  "name": "Extension Template",
  "version": "1.0",
  "description": "A clean template for Chrome extensions with rounded corners",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {},
  "background": {
    "service_worker": "service-worker.js"
  },
  "web_accessible_resources": [{
    "resources": ["iframe.html"],
    "matches": ["<all_urls>"]
  }]
}